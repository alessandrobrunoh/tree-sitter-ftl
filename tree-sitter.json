{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ftl",
      "camelcase": "Ftl",
      "title": "FTL",
      "scope": "source.ftl",
      "file-types": [
        "ftl"
      ],
      "injection-regex": "^ftl$",
      "class-name": "TreeSitterFtl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "FTL grammar for tree-sitter",
    "authors": [
      {
        "name": "Alessandro Bruno",
        "email": "alessandro.brunoh@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-ftl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
